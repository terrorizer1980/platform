{
  "name": "staking",
  "version": "0.0.6",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --aot false --host 0.0.0.0",
    "build:dev": "ng build --prod --aot",
    "build": "ng build --prod --aot && sentry-cli releases -o trustwallet -p platform new -p platform $npm_package_version && sentry-cli releases -o trustwallet -p platform files $npm_package_version upload-sourcemaps \"./dist\" && sentry-cli releases -o trustwallet -p platform finalize $npm_package_version",
    "build:watch": "ng build --prod --watch",
    "postbuild": "rimraf dist/**/*.map",
    "test": "ng test --watch false && npm run e2e",
    "updateVersion": "npm version patch && git push origin master",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "engines": {
    "node": "10.16.3",
    "npm": "6.9.0"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.4",
    "@angular/common": "^8.2.4",
    "@angular/compiler": "^8.2.4",
    "@angular/core": "^8.2.4",
    "@angular/forms": "^8.2.4",
    "@angular/platform-browser": "^8.2.4",
    "@angular/platform-browser-dynamic": "^8.2.4",
    "@angular/router": "^8.2.4",
    "@fortawesome/fontawesome-free": "^5.10.2",
    "@ng-bootstrap/ng-bootstrap": "^5.1.1",
    "@ngneat/content-loader": "^4.0.1",
    "@sentry/browser": "^5.6.3",
    "@trustwallet/api": "^0.1.14",
    "@trustwallet/provider": "^0.1.14",
    "@trustwallet/rpc": "^0.1.35",
    "@trustwallet/types": "^0.1.14",
    "@trustwallet/walletconnect": "^0.1.14",
    "@walletconnect/qrcode-modal": "^1.0.0-beta.36",
    "ajv": "^6.10.2",
    "bignumber.js": "^9.0.0",
    "bootstrap": "^4.3.1",
    "core-js": "^2.5.4",
    "express": "^4.17.1",
    "font-awesome": "^4.7.0",
    "i": "^0.3.6",
    "loaders.css": "^0.1.2",
    "localforage": "^1.7.3",
    "moment": "^2.24.0",
    "nanoid": "^2.1.2",
    "ngforage": "^5.0.1",
    "ngx-loaders-css": "^1.0.2",
    "ngx-mask": "6.5.18",
    "npm": "^6.12.0",
    "pouchdb": "^7.1.1",
    "roboto-fontface": "^0.10.0",
    "rxjs": "^6.5.3",
    "tslib": "^1.9.0",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.2",
    "@angular/cli": "^8.3.2",
    "@angular/compiler-cli": "^8.2.4",
    "@angular/language-service": "^8.2.4",
    "@sentry/cli": "^1.47.1",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/moment": "^2.13.0",
    "@types/pouchdb": "^6.4.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.10.0",
    "codelyzer": "^5.1.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.3.0",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "node-sass": "^4.12.0",
    "prettier": "^1.18.2",
    "protractor": "~5.4.0",
    "protractor-console-plugin": "^0.1.1",
    "replace-in-file": "^4.1.3",
    "rimraf": "^3.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.5.3"
  }
